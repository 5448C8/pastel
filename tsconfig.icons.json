{
  "compilerOptions": {
    "incremental": true,
    "composite": true,
    "target": "esnext",
    "jsx": "react-jsx",
    "module": "esnext",
    "declaration": true,
    "sourceMap": true,
    "importHelpers": true,
    "importsNotUsedAsValues": "remove",
    "downlevelIteration": false,
    "esModuleInterop": true,
    "inlineSources": true,
    "strict": true,
    "skipLibCheck": true,
    "emitDeclarationOnly": true,
    "moduleResolution": "node"
  },
  "include": [
    "icons/**/*"
  ]
}

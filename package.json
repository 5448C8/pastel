{
  "name": "@indaq/pastel",
  "version": "1.0.1",
  "main": "./build/index.js",
  "license": "MIT",
  "sideEffects": [
    "./build/global.js"
  ],
  "files": [
    "./build/**/*",
    "./icons/*.js",
    "./icons/*.map",
    "./icons/*.d.ts"
  ],
  "scripts": {
    "clean": "rm -rf ./build tsconfig.build.tsbuildinfo",
    "build:ts": "tsc --project tsconfig.build.json && tsc --project tsconfig.icons.json",
    "build:babel": "ts-node --skipProject scripts/build.ts",
    "build": "yarn clean && yarn build:ts && yarn build:babel",
    "build:dev": "yarn build:ts && yarn build:babel",
    "docs:dev": "next dev",
    "docs:build": "next build && next export",
    "prepare": "yarn build"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.9",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@babel/types": "^7.17.0",
    "@linaria/babel-preset": "^3.0.0-beta.15",
    "@linaria/shaker": "^3.0.0-beta.15",
    "@types/babel__core": "^7.1.19",
    "@types/node": "^17.0.25",
    "@types/react": "^18.0.5",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "eslint": "^8.13.0",
    "eslint-config-a1motion": "^3.5.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-formatter-pretty": "^4.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "globby": "^11.1.0",
    "next": "^12.1.5",
    "postcss": "^8.4.12",
    "postcss-custom-properties": "^12.1.7",
    "prettier": "^2.6.2",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "@linaria/atomic": "^3.0.0-beta.15",
    "@linaria/core": "^3.0.0-beta.15",
    "@stitches/core": "^1.2.7"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "ie >= 11"
    ],
    "development": [
      "last 1 Chrome versions"
    ]
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  }
}
